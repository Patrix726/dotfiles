# tab_bar_edge                      bottom
tab_bar_align                       center
tab_bar_margin_width      9
tab_bar_margin_height     7 0
tab_bar_style             separator
tab_bar_min_tabs          2
tab_separator             ""
tab_title_template        "{fmt.fg._5c6370}{fmt.bg.default}{fmt.fg._abb2bf}{fmt.bg._5c6370} {title[title.rfind('/')+1:]} {fmt.fg._5c6370}{fmt.bg.default} "
active_tab_title_template "{fmt.fg._e5c07b}{fmt.bg.default}{fmt.fg._282c34}{fmt.bg._e5c07b} {title[title.rfind('/')+1:]} {fmt.fg._e5c07b}{fmt.bg.default} "

# tab_bar_background           #36a3d9
# inactive_tab_background   #36a3d9
# inactive_tab_foreground    #0e1419
# active_tab_background      #0e1419
# active_tab_foreground       #E7C547

# Navigate tabs by nums
map command+1                     goto_tab 1
map command+2                     goto_tab 2
map command+3                     goto_tab 3
map command+4                     goto_tab 4
map command+5                     goto_tab 5
map command+6                     goto_tab 6
map command+7                     goto_tab 7
map command+8                     goto_tab 8
map command+9                     goto_tab 9
map command+10                    goto_tab 1
